{
    "name": "pandawa/reloquent",
    "type": "library",
    "description": "Data Mapper ORM for Pandawa",
    "keywords": [
        "pandawa",
        "laravel",
        "orm",
        "eloquent"
    ],
    "homepage": "http://github.com/pandawa/reloquent",
    "license": "MIT",
    "authors": [
        {
            "name": "Iqbal Maulana",
            "email": "iq.bluejack@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "pandawa/pandawa": "^4.0.9"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "files": [],
        "psr-4": {
            "Pandawa\\Reloquent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pandawa\\Reloquent\\Test\\": "tests/"
        }
    },
    "scripts": {
        "tests": [
            "vendor/bin/phpunit --verbose"
        ],
        "phpcs": [
            "vendor/bin/php-cs-fixer fix --verbose --diff --dry-run"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyse"
        ],
        "psalm": [
            "vendor/bin/psalm"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        },
        "laravel": {
            "providers": [
                "Pandawa\\Reloquent\\PandawaReloquentModule"
            ]
        }
    }
}
